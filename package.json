{
  "name": "raccoon",
  "version": "1.0.0",
  "description": "Raccoon is a personal wordpress starter theme based on NPM, Babel ES-2015, Gulp and Knacss",
  "main": "index.php",
  "scripts": {
    "postinstall": "npm run gulpfile-update && gulp",
    "prestart": "gulp",
    "start": "gulp watch",
    "test": "npm run jshint",
    "gulpfile-update": "babel gulpfile.babel.js --out-file gulpfile.js",
    "jshint": "jshint --reporter=node_modules/jshint-stylish --exclude-path=.gitignore . || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiwelo/raccoon.git"
  },
  "keywords": [
    "wordpress",
    "starter",
    "theme"
  ],
  "author": "Damien Senger <hi@hiwelo.co>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hiwelo/raccoon/issues"
  },
  "homepage": "https://github.com/hiwelo/raccoon#readme",
  "dependencies": {
    "jquery": "^2.1.4",
    "knacss": "^4.3.6"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.10.0",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^1.0.1",
    "gulp-filesize": "0.0.6",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^1.1.0",
    "gulp-newer": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "^1.4.2",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "yargs": "^3.31.0"
  }
}
